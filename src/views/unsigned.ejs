<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Unsigned file upload</title>
    <link rel="stylesheet" href="styles.css" />
    <script src="script.js" defer></script>
  </head>
  <body>
    <!-- my dropbox -->
    <div class="dropbox" id="dropbox">
      <h1>Drop files here to upload ğŸ“</h1>
      <input
        type="file"
        name="file-input"
        id="file-input"
        multiple
        accept=".png,.jpg,.jpeg"
      />

      <button id="img-upload-btn">Submit</button>
    </div>

    <!-- my images preview section -->
    <ul id="preview" class="preview"></ul>
  </body>
</html>
